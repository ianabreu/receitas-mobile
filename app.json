{
  "expo": {
    "name": "receitas",
    "slug": "receitas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "androidNavigationBar": {
      "visible": true,
      "barStyle": "dark-content",
